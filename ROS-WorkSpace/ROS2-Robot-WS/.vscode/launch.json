      // Example lldb launch of a ros executable
      {
        "name": "(lldb) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/install/lib/${input:package}/${input:program}",
        "args": [],
        "stopAtEntry": true,
        "cwd": "${workspaceFolder}",
        "externalConsole": false,
        "MIMode": "lldb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for lldb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    ],
    "inputs": [
      {
        "id": "package",
        "type": "promptString",
        "description": "Package name",
        "default": "robot"
      },
      {
        "id": "program",
        "type": "promptString",
        "description": "Program name",
        "default": "robot"
      }
    ]
  }